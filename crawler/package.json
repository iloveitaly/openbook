{
  "name": "crawler",
  "version": "0.0.1",
  "description": "This is an example of a Crawlee project.",
  "author": "It's not you it's me",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "dependencies": {
    "crawlee": "^3.4.0",
    "follow-redirect-url": "file:~/Projects/javascript/follow-redirect-url",
    "langchain": "^0.0.102",
    "loglevel": "^1.8.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "mysql2": "^3.4.4",
    "playwright": "*",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@apify/tsconfig": "^0.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/node": "^20.3.3",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-gitignore": "^0.1.0",
    "prettier": "^2.8.8",
    "require-in-the-middle": "^7.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "typesync": "^0.11.1",
    "better-node-inspect": "file:~/Projects/javascript/better-node-inspect"
  },
  "scripts": {
    "start": "npm run start:dev",
    "start:prod": "node dist/main.js",
    "start:dev": "ts-node-esm -T src/main.ts",
    "build": "tsc",
    "test": "echo \"Error: oops, the actor has no tests yet, sad!\" && exit 1"
  },
  "prettier": {
    "semi": false,
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderGroupNamespaceSpecifiers": true,
    "importOrder": [
      "^[:alpha].*$",
      "^@remix(.*)$",
      "^(@mantine|@emotion|@nivo)(.*)$",
      "^@prisma(.*)$",
      "^~/",
      "./"
    ]
  },
  "eslintIgnore": [
    "/bin",
    "/prisma/seed.ts",
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "typesync": {
    "ignorePackages": [
      "eslint",
      "prettier",
      "prettier-plugin-sort-imports"
    ]
  },
  "engines": {
    "node": "20.3.1",
    "npm": "9.6.7"
  }
}
