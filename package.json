{
  "name": "openbook",
  "version": "0.0.1",
  "description": "This is an example of a Crawlee project.",
  "author": "Michael Bianco <mike@mikebian.co>",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "crawlee": "^3.4.2",
    "follow-redirect-url": "github:iloveitaly/follow-redirect-url#custom",
    "hast-util-is-element": "^2.1.3",
    "langchain": "^0.0.118",
    "loglevel": "^1.8.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-gfm-table": "^2.0.0",
    "mysql2": "^3.5.2",
    "playwright": "^1.36.2",
    "rehype-parse": "^8.0.4",
    "rehype-remark": "^9.1.2",
    "rehype-remove-comments": "^5.0.0",
    "rehype-remove-images": "^1.1.1",
    "rehype-stringify": "^9.0.3",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "remark-stringify": "^10.0.3",
    "rocket-reach": "github:stainless-sdks/rocket-reach-node",
    "tiny-invariant": "^1.3.1",
    "unified": "^10.1.2",
    "unist-util-filter": "^5.0.0",
    "unist-util-remove": "^4.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@apify/tsconfig": "^0.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/node": "^20.4.5",
    "better-node-inspect": "^0.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-gitignore": "^0.1.0",
    "prettier": "^2.8.8",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "typesync": "^0.11.1",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  },
  "scripts": {
    "start": "npm run start:dev",
    "start:prod": "node dist/commands.js",
    "start:dev": "tsx -T commands.ts",
    "build": "tsc",
    "test": "vitest"
  },
  "prettier": {
    "semi": false,
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderGroupNamespaceSpecifiers": true,
    "importOrder": [
      "^[:alpha].*$",
      "^@remix(.*)$",
      "^(@mantine|@emotion|@nivo)(.*)$",
      "^@prisma(.*)$",
      "^~/",
      "./"
    ]
  },
  "eslintIgnore": [
    "/bin",
    "/prisma/seed.ts",
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "typesync": {
    "ignorePackages": [
      "eslint",
      "prettier",
      "prettier-plugin-sort-imports"
    ]
  },
  "engines": {
    "node": "20.3.1",
    "npm": "9.6.7"
  }
}
