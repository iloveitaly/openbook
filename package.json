{
  "name": "openbook",
  "version": "0.0.1",
  "description": "This is an example of a Crawlee project.",
  "author": "Michael Bianco <mike@mikebian.co>",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "crawlee": "^3.5.4",
    "follow-redirect-url": "github:iloveitaly/follow-redirect-url#custom",
    "hast-util-is-element": "^2.1.3",
    "langchain": "^0.0.150",
    "loglevel": "^1.8.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-gfm-table": "^2.0.0",
    "mysql2": "^3.6.1",
    "playwright": "^1.38.0",
    "rehype-parse": "^8.0.4",
    "rehype-remark": "^9.1.2",
    "rehype-remove-comments": "^5.0.0",
    "rehype-remove-images": "^1.1.1",
    "rehype-stringify": "^9.0.3",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "remark-stringify": "^10.0.3",
    "tiny-invariant": "^1.3.1",
    "unified": "^10.1.2",
    "unist-util-filter": "^5.0.1",
    "unist-util-remove": "^4.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@apify/tsconfig": "^0.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "better-node-inspect": "^0.3.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-gitignore": "^0.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2",
    "typesync": "^0.11.1",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.4"
  },
  "scripts": {
    "start": "npm run start:dev",
    "start:prod": "node dist/commands.js",
    "start:dev": "tsx -T commands.ts",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "format": "prettier --write .",
    "build": "tsc",
    "test": "vitest"
  },
  "prettier": {
    "semi": false,
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderGroupNamespaceSpecifiers": true,
    "importOrder": [
      "^[:alpha].*$",
      "^@remix(.*)$",
      "^(@mantine|@emotion|@nivo)(.*)$",
      "^@prisma(.*)$",
      "^~/",
      "./"
    ]
  },
  "typesync": {
    "ignorePackages": [
      "eslint",
      "prettier",
      "prettier-plugin-sort-imports"
    ]
  },
  "engines": {
    "node": "20.3.1",
    "npm": "9.6.7"
  }
}
